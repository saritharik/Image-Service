@model ImageServiceWebApp.Models.PhotosModel
@{
    ViewBag.Title = "Photos";
}

<h2>Photos</h2>
@foreach (var photo in Model.PhotosNames)
{
    <li>@photo</li>
    <div class="row">
        <div class="col-xs-6 col-md-3">
            <a href="#" class="thumbnail">
                <img src="@photo" class="img-circle" alt="Sample Photo" width="304" height="236">
            </a>
            <div class="caption">
                <p>

                <a href="#" class="btn btn-primary" role="button">Button</a>
                <a href="#" class="btn btn-default" role="button">Button</a>
                </p>
            </div>
        </div>
    </div>
    
}



